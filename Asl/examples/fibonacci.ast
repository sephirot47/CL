(LIST_FUNCTIONS (func main PARAMS (LIST_INSTR (write "Enter ther order of the Fibonacci number: ") (read n) (write "Iterative or recursive algorithm (0 or 1)? ") (read alg) (if (= alg 0) (LIST_INSTR (:= fib (FUNCALL fib_iter (ARGLIST n)))) (LIST_INSTR (:= fib (FUNCALL fib_rec (ARGLIST n))))) (write "Fibonacci(") (write n) (write ")=") (write fib) (write "%n"))) (func fib_iter (PARAMS n) (LIST_INSTR (:= i 1) (:= f_i 1) (:= f_i1 1) (while (< i n) (LIST_INSTR (:= f (+ f_i f_i1)) (:= f_i1 f_i) (:= f_i f) (:= i (+ i 1)))) (return f))) (func fib_rec (PARAMS n) (LIST_INSTR (if (<= n 1) (LIST_INSTR (return 1)) (LIST_INSTR (return (+ (FUNCALL fib_rec (ARGLIST (- n 1))) (FUNCALL fib_rec (ARGLIST (- n 2))))))))))